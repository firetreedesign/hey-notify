{
    "$schema": "https://playground.wordpress.net/blueprint-schema.json",
    "landingPage": "/wp-admin/edit.php?post_type=hey_notify",
    "preferredVersions": {
        "php": "latest",
        "wp": "latest"
    },
    "phpExtensionBundles": [
        "kitchen-sink"
    ],
    "features": {
        "networking": false
    },
    "steps": [
        {
            "step": "login",
            "username": "admin",
            "password": "password"
        },
        {
            "step": "installPlugin",
            "pluginZipFile": {
                "resource": "wordpress.org/plugins",
                "slug": "hey-notify"
            },
            "options": {
                "activate": true
            }
        },
        {
            "step": "importWxr",
            "file": {
                "resource": "url",
                "url": "https://heynotifywp.com/wp-content/uploads/2024/07/hey-notify-live-preview.xml"
            }
        }
    ]
}